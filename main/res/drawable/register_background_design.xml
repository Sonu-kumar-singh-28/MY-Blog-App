<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item
        android:top="4dp"
        android:left="4dp"
        android:right="4dp"
        android:bottom="4dp">
        <shape>
            <gradient
                android:startColor="#6151c4"
                android:centerColor="#7e6ee8"
                android:endColor="#b39ddb"
                android:angle="45"/>
            <corners
                android:radius="20dp"/>
        </shape>
    </item>

</layer-list>
